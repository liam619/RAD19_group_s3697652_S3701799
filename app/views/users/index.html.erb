<% provide(:title, "All Coordinators") %>
<div class="btm-line">
  <h1><%= yield :title %></h1>
</div>

<%= render 'shared/navbar' %>
  <table class="table table-hover">
    <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Join Date</th>
    </tr>
    </thead>

    <% @users.each do |u| %>
      <tr>
        <td><%= gravatar_for u %><%= u.name %></td>
        <td><%= u.email %></td>
        <td><%= u.created_at.strftime("%d/%m/%Y") %></td>
      </tr>
    <% end %>
  </table>


